CREATE TABLE STUDENT (DNI VARCHAR NOT NULL, NAME VARCHAR, SURNAMES VARCHAR, PRIMARY KEY (DNI))
CREATE TABLE SUBJECT (NAME VARCHAR NOT NULL, PRIMARY KEY (NAME))
CREATE TABLE STUDENT_SUBJECT (students_DNI VARCHAR NOT NULL, subjects_NAME VARCHAR NOT NULL, PRIMARY KEY (students_DNI, subjects_NAME))
ALTER TABLE STUDENT_SUBJECT ADD CONSTRAINT FK_STUDENT_SUBJECT_subjects_NAME FOREIGN KEY (subjects_NAME) REFERENCES SUBJECT (NAME)
ALTER TABLE STUDENT_SUBJECT ADD CONSTRAINT FK_STUDENT_SUBJECT_students_DNI FOREIGN KEY (students_DNI) REFERENCES STUDENT (DNI)
